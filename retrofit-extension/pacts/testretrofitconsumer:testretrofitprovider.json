{
  "provider": {
    "name": "testretrofitprovider"
  },
  "consumer": {
    "name": "testretrofitconsumer"
  },
  "interactions": [
    {
      "description": "POST example",
      "request": {
        "method": "POST",
        "path": "/api/v1/example",
        "headers": {
          "Content-Type": "application/json; charset\u003dUTF-8"
        },
        "body": {
          "def": "01.01.2000"
        },
        "matchingRules": {
          "body": {
            "$.def": {
              "matchers": [
                {
                  "match": "regex",
                  "regex": "[0-9]{2}\\.[0-9]{2}\\.[0-9]{4}"
                }
              ],
              "combine": "AND"
            }
          }
        }
      },
      "response": {
        "status": 200,
        "body": {}
      }
    }
  ],
  "metadata": {
    "pact-specification": {
      "version": "3.0.0"
    },
    "pact-laminat-android": {
      "version": "3.5.9"
    }
  }
}